{
  "name": "oneapi-ce",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:watch": "nx run-many --target=build --all --args='--watch'",
    "build:prod": "nx run-many --target=build --all --args='--prod'",
    "publish": "nx run-many --target=publish --all",
    "prepare": "husky install",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "lintfix": "nx workspace-lint && nx run-many --target=lint --all --args='--fix'",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --args='--watch'",
    "test:coverage": "nx run-many --target=test --all --args='--code-coverage'",
    "test:ci": "nx run-many --target=test --all --parallel=1 --args='--code-coverage'",
    "test:debug": "nx run-many --target=test --all --args='--inspect-brk'",
    "test:e2e": "nx run-many --target=e2e --all"
  },
  "private": true,
  "lint-staged": {
    "*.{js,ts}": "nx workspace-lint && nx lint"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "axios": "^1.3.2",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.1.1",
    "firebase-admin": "^10.0.2",
    "firebase-backend": "^0.2.5",
    "firebase-functions": "^3.18.0",
    "itty-router": "^3.0.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tough-cookie": "^4.1.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230115.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/eslint-plugin-nx": "15.6.3",
    "@nrwl/jest": "15.6.3",
    "@nrwl/js": "15.6.3",
    "@nrwl/linter": "15.6.3",
    "@nrwl/nest": "15.6.3",
    "@nrwl/node": "15.6.3",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/vite": "15.6.3",
    "@nrwl/webpack": "15.6.3",
    "@nrwl/workspace": "15.6.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "28.1.1",
    "@types/jsdom": "^21.1.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitest/coverage-c8": "~0.25.8",
    "@vitest/ui": "^0.25.8",
    "eslint": "~8.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.2.0",
    "husky": "^8.0.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jsdom": "~20.0.3",
    "lint-staged": "^13.1.1",
    "nx": "15.6.3",
    "prettier": "^2.6.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2",
    "vite": "^4.0.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.25.8",
    "wrangler": "2.9.1"
  }
}
